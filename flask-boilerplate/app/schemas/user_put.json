{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "user_put.json",
	"type": "object",
	"title": "/user PUT endpoint schema",
	"required": ["first_name", "last_name", "date_of_birth"],
	"properties": {
		"first_name": {
			"type": "string",
			"title": "User First Name"
		},
		"last_name": {
			"type": "string",
			"title": "User Last Name"
		},
		"date_of_birth": {
			"type": "string",
			"format": "date",
			"title": "User Date of Birth (YYYY-MM-DD)"
		}
	},
	"additionalProperties": false
}